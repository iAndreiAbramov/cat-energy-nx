{
  "kind": "collectionType",
  "collectionName": "main_products",
  "info": {
    "singularName": "main-product",
    "pluralName": "main-products",
    "displayName": "MainProduct",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productBrand": {
      "type": "string",
      "required": true,
      "default": "Cat Energy"
    },
    "productCategory": {
      "type": "string",
      "required": true
    },
    "productWeight": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "productTaste": {
      "type": "string",
      "required": true
    },
    "productPrice": {
      "type": "decimal",
      "required": true,
      "default": 0
    },
    "image": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    }
  }
}
